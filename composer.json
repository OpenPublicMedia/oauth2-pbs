{
    "name": "openpublicmedia/oauth2-pbs",
    "description": "PBS OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Charbonneau Wells",
            "email": "openpublicmedia-oauth2-pbs@chris-wells.net",
            "homepage": "https://www.chris-wells.net"
        }
